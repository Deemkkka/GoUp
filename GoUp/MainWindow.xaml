<Window x:Class="GoUp.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GoUp"
        mc:Ignorable="d"
        Title="Movement" Height="450" Width="800">
    <Canvas Name="MyCanvas" Background="LightBlue" Focusable="True" KeyDown="KeyIsDown" KeyUp="KeyIsUp">

        <Rectangle Name="player" Width="50" Height="50" Fill="Red" Canvas.Left="20" Canvas.Top="50"/>
        <Rectangle Name="box" Width="50" Height="50" Fill="Black" Canvas.Left="200" Canvas.Top="250"/>
        <Rectangle Name="box2" Width="50" Height="50" Fill="Black" Canvas.Left="300" Canvas.Top="50"/>
        <Rectangle Name="box3" Width="50" Height="50" Fill="Black" Canvas.Left="400" Canvas.Top="150"/>
        <Rectangle Name="box4" Width="50" Height="50" Fill="Black" Canvas.Left="500" Canvas.Top="350"/>
    </Canvas>
</Window>
